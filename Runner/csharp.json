{
	"id": "csharp",
	"version": "0.10.6",
	"description": "C# support for gauge",
	"PostInstall": {
		"windows": [".\\deprecation.bat"],
		"linux": ["bash", "setup.sh"],
		"darwin": ["bash", "setup.sh"]
	},
	"run": {
		"windows": ["bin\\Gauge.Csharp.Runner.exe", "--start"],
		"linux": ["./run.sh", "--start"],
		"darwin": ["./run.sh", "--start"]
	},
	"init": {
		"windows": ["bin\\Gauge.Csharp.Runner.exe", "--init"],
		"linux": ["./run.sh", "--init"],
		"darwin": ["./run.sh", "--init"]
	},
	"gaugeVersionSupport": {
		"minimum": "1.0.6",
		"maximum": ""
	}
}

{
  "id" : "csharp",
  "version" : "0.8.1",
  "description": "C# support for gauge",
  "PostInstall": {
    "windows": [],
    "linux": ["chmod","+x", "run.sh"],
    "darwin": ["chmod","+x", "run.sh"]
  },
  "run": {
    "windows": ["bin\\Gauge.Csharp.Runner.exe", "--start"],
    "linux": ["./run.sh", "--start"],
    "darwin": ["./run.sh", "--start"]
  },
  "init": {
    "windows": ["bin\\Gauge.Csharp.Runner.exe", "--init"],
    "linux": ["./run.sh", "--init"],
    "darwin": ["./run.sh", "--init"]
  },
  "gaugeVersionSupport": {
      "minimum": "0.5.0",
      "maximum": ""
  }
}
